    }
  },
  "image#banner-50-off-UPT": {
    "props": {
      "alt": "End Of Season",
      "title": "End Of Season",
      "src": "/arquivos/HERO-Banner-Desktop-UPT-CL.jpg",
      "link": {
        "url": "/ofertas"
      },
      "blockClass": ["image-carousel3", "image-main-carousel"]
