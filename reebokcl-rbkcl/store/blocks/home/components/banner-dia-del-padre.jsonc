    "link#buttons-desktopHome-dia-del-padre": {
        "props": {
            "href": "/diadelpadre",
            "label": "COMPRAR AHORA",
            "blockClass": "buttons-desktopHome-2"
        }
    },
    "rich-text#titulo-rebook-dia-del-padre": {
        "props": {
            "blockClass": [
                "titulo-rebook-tunes",
                "titulo-rebook-dia-del-padre"
