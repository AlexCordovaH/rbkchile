        ]
    },
    "flex-layout.row#body-desktopHome-dia-del-padre": {
        "props": {
            "blockClass": "body-desktopHome"
        },
        "children": [
            "rich-text#body-rebook-tunes-dia-del-padre"
        ]
    },

    "flex-layout.row#body-mobileHome-dia-del-padre": {
        "props": {
            "blockClass": "body-desktopHome"
        },
        "children": [
            "rich-text#body-rebook-tunes-dia-del-padre"
        ]
