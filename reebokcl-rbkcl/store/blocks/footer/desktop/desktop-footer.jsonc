  "flex-layout.row#social-media": {
    "props": {
      "fullWidth": true,
      "horizontalAlign":"center",
      "preserveLayoutOnMobile":true,
      "blockClass": [
        "footer_social_media"
      ]
    },
    "children": [
      "image#footer-social-media-fb",
      "image#footer-social-media-ig",
      "image#footer-social-media-yt"
    ]
  },
  "image#footer-social-media-fb": {
    "props": {
      "src": "assets/footer/facebook.svg",
      "alt":"Facebook | Reebok Chile",
      "title":"Facebook Reebok Chile",
      "maxHeight":18,
      "maxWidth":10,
      "blockClass":"footer_img_social_media",
      "link":{
        "newTab":true,
        "url":"https://www.facebook.com/ReebokChile"
      }
    }
  },
  "image#footer-social-media-ig": {
    "props": {
      "src": "assets/footer/instagram.svg",
      "alt":"Instagram | Reebok Chile",
      "title":"Instagram Reebok Chile",
