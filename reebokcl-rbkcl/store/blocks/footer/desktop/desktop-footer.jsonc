{
  "footer-layout.desktop": {
    "children": [
      // "flex-layout.row#footer-desktop-menu"
      // "back-to-top-button",
      "flex-layout.row#container-footer-row-newsletter",
      "flex-layout.row#desktop-footer-row-one",
      "flex-layout.row#desktop-footer-row-two"
    ]
  },
  "flex-layout.row#desktop-footer-row-one": {
    "children": [
      "flex-layout.col#col-footer-desktop-1",
      "flex-layout.col#col-footer-desktop-2",
      "flex-layout.col#col-footer-desktop-3",
      "flex-layout.col#col-footer-desktop-4"
      //"flex-layout.col#col-footer-desktop-5"
    ],
    "props": {
      "blockClass": "desktop-footer-one"
    }
  },
  "flex-layout.col#col-footer-desktop-1": {
    "children": [
      "rich-text#message-newsletter-footer",
     // "modal-trigger#newsletter",
      "flex-layout.row#social-media"
    ],
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "top",
      "width": "25%",
      "marginLeft": 8,
      "marginRight": 8,
      "blockClass": "newsletter-footer"
    }
  },
  "flex-layout.col#col-footer-desktop-2": {
    "children": [
      "vtex.menu@2.x:menu#desktop-productos"
    ],
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "top",
      "width": "17%",
      "marginLeft": 8,
      "marginRight": 8
    }
  },
  "flex-layout.col#col-footer-desktop-3": {
    "children": [
      "vtex.menu@2.x:menu#desktop-servicio-al-cliente"
    ],
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "top",
      "width": "17%",
      "marginLeft": 8,
      "marginRight": 8
    }
  },
  "flex-layout.col#col-footer-desktop-4": {
    "children": [
      "flex-layout.col#menu-item-footer-acerca-de-buscador-de-tiendas"
    ],
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "top",
      "width": "17%",
      "marginLeft": 8,
      "marginRight": 8
    }
  },
  "flex-layout.col#col-footer-desktop-5": {
    "children": [
      "flex-layout.col#col-5-footer-desktop"
    ],
    "props": {
      "horizontalAlign": "right",
      "verticalAlign": "top",
      "width": "17%",
      "marginLeft": 8,
      "marginRight": 8
    }
  },
  "flex-layout.row#desktop-footer-row-two": {
    "props": {
      "horizontalAlign": "center",
      "fullWidth": true,
      "blockClass": "desktop-footer-two"
    },
    "children": [
      "flex-layout.col#desktop-footer-copyright",
      "flex-layout.col#desktop-footer-links-row-two"
    ]
  },
  "flex-layout.col#desktop-footer-copyright": {
    "props": {
      "marginLeft": 8,
      "marginRight": 8
    },
    "children": [
      "footer-year-copyright"
    ]
  },
  "footer-year-copyright":{
    "props": {
      "beforeTextYear": "Reebok™ es una marca registrada de Reebok International Limited.©",
      "afterTextYear": "Reebok International Limited. Todos los derechos reservados."
    }
  },
  "flex-layout.col#desktop-footer-links-row-two": {
    "props": {
      "horizontalAlign": "right",
      "marginLeft": 8,
      "marginRight": 8
    },
    "children": [
      "rich-text#links-footer-desktop-row-two"
    ]
  },
  "rich-text#links-footer-desktop-row-two": {
    "props": {
      "text": "[Politicas de Privacidad](/politicas-de-privacidad) [Términos y Condiciones](/terminos-condiciones)",
      "blockClass": "links-footer-desktop-row-two"
    }
  },
  // NewsLetter Footer
  "rich-text#message-newsletter-footer": {
    "props": {
      "text": "SIGUENOS EN NUESTRAS REDES SOCIALES.",
      "blockClass": "newsletter-title-footer"
    }
  },
  "modal-trigger#newsletter": {
    "children": [
      "rich-text#newsletter-action-button",
      "modal-layout#newsletter-layout-footer"
    ]
  },
  "modal-header#newsletter": {
    "props": {
      "showCloseButton": true,
      "blockClass": "newsletter-header"
    }
  },
  "rich-text#newsletter-action-button": {
    "props": {
      "text": "[Suscribirme]()",
      "blockClass": "action-button-newsletter-footer"
    }
  },
  "modal-layout#newsletter-layout-footer": {
    "props": {
      "blockClass": "modal-layout-newsletter-footer"
    },
    "children": [
      "modal-header#newsletter",
      "flex-layout.row#logo-newsletter-content"
    ]
  },

  "flex-layout.row#logo-newsletter-content":{
    "children":[
      "image#logo-newsletter-content",
      "flex-layout.col#logo-newsletter-content2"
    ],
    "props":{
      "blockClass": "row-content-newsletter"
    }
  },

  "flex-layout.col#logo-newsletter-content1":{
    "children":["image#logo-newsletter-content"
    ],
    "props":{
            "width": "47%",
            "blockClass": "col-image-newsletter"
    }
  },
  
  "image#logo-newsletter-content":{
    "props":{
      "blockClass": "img-newsletter-content",
      "src": "/arquivos/BENEFICIOS-CRM-MODAL.jpg"
    }
  },

  "flex-layout.col#logo-newsletter-content2":{
    "children":[     
    "flex-layout.row#logo-newsletter",
    "flex-layout.row#newsletter-text-footer",
    "flex-layout.row#footer-newsletter-mobile"],
    "props":{
      "width": "55%"
    }
  },

  "flex-layout.row#newsletter-text-footer": {
    "props": {
      "blockClass": "texts-modal-newsletter-footer"
    },
    "children": [
      "rich-text#newsletter-modal-title-footer",
      "rich-text#newsletter-modal-paragraph-footer"
    ]
  },
  "rich-text#newsletter-modal-title-footer": {
    "props": {
      "text": "#### SUSCRÍBETE Y ENTÉRATE DE TODAS NUESTRAS NOVEDADES",
      "blockClass": "footer-newsletter-title-modal"
    }
  },
  "rich-text#newsletter-modal-paragraph-footer": {
    "props": {
      "text": "Únete a nuestro newsletter y recibe 20% de descuento en tu próxima compra",
      "blockClass": "footer-newsletter-paragraph-modal"
    }
  },
  "flex-layout.row#footer-newsletter-mobile": {
    "props": {
      "blockClass": "footer-newsletter-mobile",
      "fullWidth": true
    },
    "children": [
      "flex-layout.col#footer-newsletter-mobile-app"
    ]
  },
  "flex-layout.col#footer-newsletter-mobile-app": {
    "props": {
      "blockClass": "footer-newsletter",
      "fullWidth": true,
      "preventVerticalStretch": true,
      "horizontalAlign": "left"
    },
    "children": [
      "formulario#newsletter"
    ]
  },
