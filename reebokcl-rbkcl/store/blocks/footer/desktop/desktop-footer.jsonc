      "maxWidth":18,
      "blockClass":"footer_img_social_media",
      "link":{
        "newTab":true,
        "url":"https://www.instagram.com/reebokchile/"
      }
    }
  },
  "image#footer-social-media-yt": {
    "props": {
      "src": "assets/footer/youtube.svg",
      "alt":"Youtube | Reebok Chile",
      "title":"Youtube Reebok Chile",
      "maxHeight":13,
      "maxWidth":21,
      "blockClass":"footer_img_social_media",
      "link":{
        "newTab":true,
        "url":"https://www.youtube.com/channel/UC0gcFrwAQ8CtzP_NSFIZtiw"
      }
    }
  },
